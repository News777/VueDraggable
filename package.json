{
  "name": "vue-auto-draggable",
  "author": "News777",
  "version": "1.0.7",
  "license": "MIT",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE",
    "./src/env.d.ts"
  ],
  "main": "./dist/vue-auto-draggable.umd.js",
  "module": "./dist/vue-auto-draggable.es.js",
  "exports": {
    "./dist/style.css": "./dist/style.css",
    "./css": "./dist/style.css",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-auto-draggable.es.js",
      "require": "./dist/vue-auto-draggable.umd.cjs"
    }
  },
  "keywords": [
    "vue3",
    "auto",
    "draggable",
    "plugin"
  ],
  "types": "./dist/index.d.ts",
  "description": "An element-dragable plug-in based on Vue3.4+Ts5.x",
  "private": false,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/News777/VueDraggable.git"
  },
  "bugs": {
    "url": "https://github.com/News777/VueDraggable/issues"
  },
  "homepage": "https://github.com/News777/VueDraggable#readme",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force"
  },
  "dependencies": {
    "decimal.js": "^10.4.3",
    "lodash": "^4.17.21",
    "vue": "^3.3.11",
    "vue-auto-draggable": "^1.0.2"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/tsconfig": "^0.5.0",
    "npm-run-all2": "^6.1.1",
    "sass": "^1.69.7",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.7.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-tsc": "^1.8.25"
  },
  "engines": {
    "node": ">= 18.0.0",
    "npm": ">= 9.0.0"
  }
}
